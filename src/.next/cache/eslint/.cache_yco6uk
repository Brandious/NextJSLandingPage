  React.useEffect(() => {\n      // wait for the useRouter hook to asynchronously get the query i\n      const fetchUser = async () => {\n        \n        try{\n          setLoading(true);\n          const response = await fetch(`https://dummyapi.io/data/v1/user/${newid}`, {\n          method: \"GET\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n            \"app-id\":\"6313d37dac3ee806bffc0c82\"\n          },\n        });\n  \n        if (!response.ok) {\n          throw new Error(`Error: ${response.status}`);\n        }\n        const user1 = await response.json();\n\n        setUser({ ...user1,dateOfBirth: formatDate(user1.dateOfBirth),registerDate: formatDate(user1.registerDate)});\n        setLoading(false);\n      }\n      catch(err)\n      {\n          console.log(err.message);\n      }\n  \n        \n      }\n  \n      fetchUser();\n\n      \n    }, [newid]);\n  \n\n\n    const toggle = () =>{\n         setId(id);\n         setModal(!modal);\n    }\n\n\nif(loading) return <p>Loading...</p>\nreturn (\n    <Card\n  style={{\n    width: '18rem',\n     \n  }}\n>\n  <img\n    alt=\"Sample\"\n    src={picture}\n    \n  />\n  <CardBody>\n    <CardTitle tag=\"h5\">\n      {title} {firstName} {lastName}\n    </CardTitle>\n    <Button color=\"danger\" onClick={toggle}>\n        Click Me\n      </Button>\n      <Modal isOpen={modal} toggle={toggle} >\n        <ModalHeader toggle={toggle}>{user.title + \" \" + user.firstName + \" \" + user.lastName}</ModalHeader>\n        <ModalBody style={{display:'flex', flexDirection:'column'}}>\n            <img src={user.picture} alt={'user'} />     \n            <span>{user.dateOfBirth}</span>\n            <span>{user.gender}</span>\n            <span>{user.phone}</span>\n            <span>{user.registerDate}</span>\n\n        </ModalBody>\n        <ModalFooter>\n          <Button color=\"secondary\" onClick={toggle}>\n            Cancel\n          </Button>\n        </ModalFooter>\n      </Modal>\n  </CardBody>\n</Card>\n  );\n}\n","/home/brandious/Dev/loydsDigital/NextJSLandingPage/src/components/Covid.jsx",[],[],"/home/brandious/Dev/loydsDigital/NextJSLandingPage/src/components/Footer.jsx",[],[],"/home/brandious/Dev/loydsDigital/NextJSLandingPage/src/components/Header.jsx",[],[],"/home/brandious/Dev/loydsDigital/NextJSLandingPage/src/components/Hero.jsx",[],[],"/home/brandious/Dev/loydsDigital/NextJSLandingPage/src/components/Layout.jsx",["86"],[],"import React from 'react';\nimport Head from 'next/head';\n\nconst Layout = (props) => {\n    return (\n        <div>\n            <Head>\n                <title>{props.pageTitle}</title>\n                <meta charSet=\"UTF-8\" />\n                <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n                <meta httpEquiv=\"X-UA-Compatible\" content=\"ie=edge\" />\n                <link\n                    rel=\"stylesheet\"\n                    href=\"https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css\"\n                />\n                <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\" />\n              \n                <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossOrigin />\n              \n                <link href=\"https://fonts.googleapis.com/css2?family=Titillium+Web&display=swap\" rel=\"stylesheet\" />\n\n            </Head>\n            <div>\n                {props.children}\n            </div>\n        </div>\n    )\n}\n\nexport default Layout;\n","/home/brandious/Dev/loydsDigital/NextJSLandingPage/src/components/New.jsx",[],[],"/home/brandious/Dev/loydsDigital/NextJSLandingPage/src/components/Services.jsx",[],[],"/home/brandious/Dev/loydsDigital/NextJSLandingPage/src/components/Services1.jsx",[],[],"/home/brandious/Dev/loydsDigital/NextJSLandingPage/src/next.config.js",[],[],"/home/brandious/Dev/loydsDigital/NextJSLandingPage/src/pages/_app.js",[],[],"/home/brandious/Dev/loydsDigital/NextJSLandingPage/src/p